# For Go Env
export GOROOT=$(pwd)/deps/go
export PATH=$PATH:$GOROOT/bin
export GOPATH=$(pwd)
export PATH=$PATH:$GOPATH/bin

# For docker Env
export MACHINE_DOCKER_INSTALL_URL=http://docker-mirror.oss-cn-hangzhou.aliyuncs.com/

# For tools Env
export PATH=$PATH:$(pwd)/tools

# Create Catalog For Redis
mkdir -p log
mkdir -p data/redis_db
mkdir -p log/redis_log

# Create Catalog For MySQL 
mkdir -p data/mysql_db
mkdir -p data/mysql_db_backup

